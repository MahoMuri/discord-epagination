{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["dist/"],
    "includePattern": ".js$"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true,
    "default": {
      "outputSourceFiles": false,
      "includeDate": false
    }
  },
  "opts": {
    "destination": "./docs/",
    "readme": "./README.md",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/jsdoc-skyceil"
  }
}
